{
  "name": "wp-post",
  "version": "0.0.1",
  "description": "Convert .md (markdown file) to html and post to wordpress.",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": {
    "wppost": "dist/bin/wp-post.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest --silent=false --verbose false"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/axios": "^0.14.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^18.15.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/probe-image-size": "^7.2.0",
    "argparse": "^2.0.1",
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.10",
    "commander": "^10.0.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^8.1.2",
    "markdown-it": "^12.3.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-expandable": "^1.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-prism": "^2.3.0",
    "markvis": "^0.0.12",
    "prettier": "^2.8.4",
    "probe-image-size": "^7.2.3",
    "sharp": "^0.30.7"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.(ts|js)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testEnvironment": "node"
  },
  "types": "./dist\\index.d.ts",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydro-cloud/wp-post.git"
  },
  "author": "tom",
  "bugs": {
    "url": "https://github.com/hydro-cloud/wp-post/issues"
  },
  "homepage": "https://github.com/hydro-cloud/wp-post#readme",
  "keywords": [
    "wordpress",
    "markdown",
    "markdown-it"
  ]
}
